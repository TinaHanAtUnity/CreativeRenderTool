    "rollup-plugin-json": "^4.0.0",
    "string-loader": "^0.0.1",
    "style-loader": "^0.23.1",
    "stylus-loader": "^3.0.2",
    "terser-webpack-plugin": "^1.4.3",
    "thread-loader": "^2.1.2",
    "to-string-loader": "^1.1.6",
    "ts-jest": "^24.3.0",
    "ts-loader": "^6.2.2",
    "tsconfig-paths-webpack-plugin": "^3.2.0",
    "typescript": "3.3.3",
    "url-loader": "^2.3.0",
    "webfont": "^9.0.0",
    "webpack": "^4.43.0",
    "webpack-bundle-analyzer": "^3.7.0",
