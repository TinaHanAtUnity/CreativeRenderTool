{
  "name": "unity-ads-webview",
  "version": "2.0.0",
  "description": "Unity Ads WebView",
  "author": "Ville Orkas",
  "repository": {
    "type": "git",
    "url": "https://github.com/Applifier/unity-ads-webview.git"
  },
  "scripts": {
    "start": "rimraf ./dist && webpack-dev-server --port 8000 --host 0.0.0.0 --config tools/webpack/server.dev.js",
    "start:release": "rimraf ./dist && NODE_ENV=production webpack-dev-server --port 8000 --host 0.0.0.0 --config tools/webpack/server.release.js",
    "build": "ANALYZE=true npm run build:release",
    "build:release": "rimraf ./dist && NODE_ENV=production webpack --config tools/webpack/config.release.js",
    "test": "jest",
    "lint-code": "git diff-index --name-only HEAD | grep -E \"(.*)\\.(ts)\" | xargs eslint",
    "lint-styles": "git diff-index --name-only HEAD | grep -E \"(.*)\\.(styl)\" | xargs stylint -c stylintrc.json",
    "purge-tags": "git tag -d $(git tag -l) >/dev/null"
  },
  "pre-commit": [
    "lint-code",
    "lint-styles",
    "purge-tags"
  ],
  "license": "Apache-2.0",
  "dependencies": {
    "@types/chai": "4.1.7",
    "@types/json-schema": "^7.0.4",
    "@types/mocha": "5.2.5",
    "@types/sinon": "4.3.3",
    "@typescript-eslint/eslint-plugin": "1.7.0",
    "@typescript-eslint/eslint-plugin-tslint": "^1.13.0",
    "@typescript-eslint/parser": "1.7.0",
    "autoprefixer-stylus": "0.14.0",
    "chai": "4.2.0",
    "chokidar": "2.1.0",
    "eslint": "5.16.0",
    "google-closure-compiler": "20190121.0.0",
    "html-minifier": "3.5.21",
    "inline-source-cli": "^2.0.0",
    "mocha": "5.2.0",
    "node-fetch": "2.3.0",
    "protobufjs": "6.8.8",
    "puppeteer": "^2.1.1",
    "puppeteer-extra": "2.1.1",
    "puppeteer-extra-plugin-user-data-dir": "2.1.1",
    "puppeteer-extra-plugin-user-preferences": "2.1.1",
    "remap-istanbul": "0.13.0",
    "rollup": "1.1.2",
    "rollup-plugin-commonjs": "9.2.0",
    "rollup-plugin-html": "0.2.1",
    "rollup-plugin-includepaths": "0.2.3",
    "rollup-plugin-istanbul": "2.0.1",
    "rollup-plugin-node-resolve": "4.0.0",
    "rollup-plugin-sourcemaps": "0.4.2",
    "rollup-plugin-string": "2.0.2",
    "sinon": "7.2.3",
    "stacktrace-js": "2.0.0",
    "stylint": "^2.0.0",
    "stylus": "0.54.5",
    "tslib": "1.9.3",
    "tslint": "5.12.1",
    "tslint-microsoft-contrib": "6.0.0",
    "typescript": "3.3.3",
    "webfont": "^9.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@babel/polyfill": "^7.8.7",
    "@types/jest": "^24.9.1",
    "babel-loader": "^8.1.0",
    "base64-inline-loader": "^1.1.1",
    "canvas": "^2.6.1",
    "css-loader": "^3.5.3",
    "eslint-loader": "^2.2.1",
    "file-loader": "^4.3.0",
    "html-loader": "^0.5.5",
