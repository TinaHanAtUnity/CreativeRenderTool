{
    "start": "ageGateDecision",
    "nodes": {
      "ageGateDecision": {
        "isSelfEvaluating": true,
        "links": [
          {
            "node": "ageSelect",
            "condition": {
              ">": [
                {
                  "var": "env.ageGateLimit"
                },
                0
              ]
            }
          },
          {
            "node": "consent",
            "condition": {
              "==": [
                {
                  "var": "env.ageGateLimit"
                },
                0
              ]
            }
          }
        ]
      },
      "ageSelect": {
        "title": "ageSelect.title",
        "subtitle": "ageSelect.subtitle",
        "components": {
          "footer": [
            {
              "text": "overAgeLimit",
              "type": "button",
              "isSubmit": true,
              "variant": "primary-privacy",
              "actions": [
                {
                  "property": "agreedOverAgeLimit",
                  "value": true
                }
              ]
            },
            {
              "text": "underAgeLimit",
              "type": "button",
              "isSubmit": true,
              "variant": "secondary-privacy",
              "actions": [
                {
                  "property": "agreedOverAgeLimit",
                  "value": false
                }
              ]
            }
          ]
        },
        "links": [
          {
            "node": "consent",
            "condition": {
              "==": [
                {
                  "var": "user.agreedOverAgeLimit"
                },
                true
              ]
            }
          }
        ]
      },
      "consent": {
        "title": "consent.title",
        "subtitle": "consent.subtitle",
        "components": {
          "footer": [
            {
              "type": "button",
              "text": "acceptTracking",
              "variant": "primary-privacy",
              "actions": [
                {
                  "property": "acceptTracking",
                  "value": true
                },
                {
                  "property": "gameExp",
                  "value": true
                },
                {
                  "property": "ads",
                  "value": true
                },
                {
                  "property": "external",
                  "value": true
                }
              ],
              "isSubmit": true
            },
            {
              "type": "button",
              "text": "showChoices",
              "variant": "secondary-privacy",
              "actions": [
                {
                  "property": "acceptTracking",
                  "value": false
                }
              ],
              "isSubmit": true
            }
          ]
        },
        "links": [
          {
            "node": "customization",
            "condition": {
              "==": [
                {
                  "var": "user.acceptTracking"
                },
                false
              ]
            }
          }
        ]
      },
      "customization": {
        "title": "consent.title",
        "subtitle": "consent.subtitle",
        "components": {
          "body": [
            {
              "type": "switch",
              "text": "allowGameExperienceDataCollection",
              "isChecked": {
                "==": [
                  {
                    "var": "user.gameExp"
                  },
                  true
                ]
              },
              "actions": [
                {
                  "property": "gameExp",
                  "value": {
                    "!": [
                      {
                        "var": [
                          "user.gameExp"
                        ]
                      }
                    ]
                  }
                }
              ]
            },
            {
              "type": "switch",
              "text": "allowGameAdsDataCollection",
              "isChecked": {
                "==": [
                  {
                    "var": "user.ads"
                  },
                  true
                ]
              },
              "actions": [
                {
                  "property": "ads",
                  "value": {
                    "!": [
                      {
                        "var": [
                          "user.ads"
                        ]
                      }
                    ]
                  }
                },
                {
                  "property": "external",
                  "value": {
                    "if": [
                      {
                        "!": [
                          {
                            "var": [
                              "user.ads"
                            ]
                          }
                        ]
                      },
                      false,
                      {
                        "var": [
                          "user.external"
                        ]
                      }
                    ]
                  }
                }
              ]
            },
            {
              "type": "switch",
              "text": "allowThirdPartyDataCollection",
              "isChecked": {
                "==": [
                  {
                    "var": "user.external"
                  },
                  true
                ]
              },
              "isDisabled": {
                "==": [
                  {
                    "var": [
                      "user.ads",
                      false
                    ]
                  },
                  false
                ]
              },
              "actions": [
                {
                  "property": "external",
                  "value": {
                    "!": [
                      {
                        "var": [
                          "user.external"
                        ]
                      }
                    ]
                  }
                },
                {
                  "property": "ads",
                  "value": {
                    "if": [
                      {
                        "!": [
                          {
                            "var": [
                              "user.external"
                            ]
                          }
                        ]
                      },
                      false,
                      {
                        "var": [
                          "user.ads"
                        ]
                      }
                    ]
                  }
                }
              ]
            }
          ],
          "footer": [
            {
              "type": "button",
              "text": "allowAll",
              "actions": [
                {
                  "property": "gameExp",
                  "value": true
                },
                {
                  "property": "ads",
                  "value": true
                },
                {
                  "property": "external",
                  "value": true
                }
              ],
              "isSubmit": true,
              "submitDelay": 1000,
              "variant": "primary-privacy",
              "isHidden": {
                "or": [
                  {
                    "==": [
                      {
                        "var": "user.gameExp"
                      },
                      true
                    ]
                  },
                  {
                    "==": [
                      {
                        "var": "user.ads"
                      },
                      true
                    ]
                  },
                  {
                    "==": [
                      {
                        "var": "user.external"
                      },
                      true
                    ]
                  }
                ]
              }
            },
            {
              "type": "button",
              "text": "disagree",
              "isSubmit": true,
              "variant": "secondary-privacy",
              "isHidden": {
                "or": [
                  {
                    "==": [
                      {
                        "var": "user.gameExp"
                      },
                      true
                    ]
                  },
                  {
                    "==": [
                      {
                        "var": "user.ads"
                      },
                      true
                    ]
                  },
                  {
                    "==": [
                      {
                        "var": "user.external"
                      },
                      true
                    ]
                  }
                ]
              }
            },
            {
              "type": "button",
              "text": "saveChoices",
              "isSubmit": true,
              "variant": "primary-privacy",
              "isHidden": {
                "and": [
                  {
                    "==": [
                      {
                        "var": [
                          "user.gameExp",
                          false
                        ]
                      },
                      false
                    ]
                  },
                  {
                    "==": [
                      {
                        "var": [
                          "user.ads",
                          false
                        ]
                      },
                      false
                    ]
                  },
                  {
                    "==": [
                      {
                        "var": [
                          "user.external",
                          false
                        ]
                      },
                      false
                    ]
                  }
                ]
              }
            }
          ]
        },
        "links": [],
        "allowGoBack": true
      }
    }
  }
