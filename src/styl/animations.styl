    -webkit-transform: scale(0.75);
    transform: scale(0.75);
  }
}

@-webkit-keyframes animateBubble {
  0% {
    -webkit-transform: translateY(200%);
    transform: translateY(200%);
  }
  100% {
    -webkit-transform: translateY(-500%);
    transform: translateY(-500%);
  }
}

@keyframes animateBubble {
  0% {
    -webkit-transform: translateY(200%);
    transform: translateY(200%);
  }
  100% {
    -webkit-transform: translateY(-500%);
    transform: translateY(-500%);
  }
}

@-webkit-keyframes flash {
  80% {
    opacity: 0;
    -webkit-transform: translate(-110%) skewX(45deg);
    transform: translate(-110%) skewX(45deg);
  }

  90% {
    opacity: 1;
  }
  100% {
    opacity: 0;
    -webkit-transform: translate(200%) skewX(45deg);
    transform: translate(200%) skewX(45deg);
  }
}

@keyframes flash {
  80% {
    opacity: 0;
    -webkit-transform: translate(-110%) skewX(45deg);
    transform: translate(-110%) skewX(45deg);
  }

  90% {
    opacity: 1;
  }
  100% {
    opacity: 0;
    -webkit-transform: translate(200%) skewX(45deg);
    transform: translate(200%) skewX(45deg);
  }
}
