#gdpr-consent {
  position: absolute;
  width: 100%;
  height: 100%;

  .consent-container {
    positon: absolute;
    width: 50%;
    height: 50%;
    margin: 25%;
    background: red;
  }

  .yes,
  .no {
    position: absolute;
    bottom: 0;
  }

  .yes {
    left: 0;
  }

  .no {
    right: 0;
  }

}
