@font-face
  font-family: "unityicons"
  src: url("../fonts/unityicons.ttf") format("truetype")

[data-icon]:before
  font-family: "unityicons"
  content: attr(data-icon)

[class^="icon-"]:before
[class*=" icon-"]:before
  font-family: "unityicons"

.icon-info:before
  content: "\61"

.icon-reload:before
  content: "\62"

.icon-star:before
  display: inline-block
  width: 30px
  content: "\65"

.icon-volume:before
  content: "\66"

.icon-volume-mute:before
  content: "\67"

.icon-skip:before
  content: "\69"

.icon-person:before
  content: "\6a"

.icon-close:before
  content: "\6c"
