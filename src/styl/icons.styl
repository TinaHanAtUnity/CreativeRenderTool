@font-face {
  font-family: "unityicons";
  src: url("../fonts/unityicons.ttf") format("truetype");
}

[data-icon]:before {
  content: attr(data-icon);
  font-family: "unityicons" !important; // @stylint ignore
}

[class^="icon-"]:before,
[class*=" icon-"]:before {
  font-family: "unityicons" !important; // @stylint ignore
}

/*
    Dummy icon that won't be rendered on android 4.2 and 4.3,
    thus always make sure that your font is packed so that first icon will
    no be visible on those old androids.
*/
.icon-android:before {
  content: "\e000";
}

.icon-star:before {
  display: inline-block;
  width: 1em;
  content: "\e001";
}

.icon-info:before {
  content: "\e002";
}

.icon-volume:before {
  content: "\e003";
}

.icon-volume-mute:before {
  content: "\e004";
}

.icon-reload:before {
  content: "\e005";
}

.icon-person:before {
  content: "\e006";
}

.icon-skip:before {
  content: "\e007";
}

.icon-close:before {
  content: "\e008";
}

.icon-screen-full:before {
  content: "\e009";
}

.icon-gdpr:before {
  content: "\e010";
}

.icon-info-ar:before {
  content: "\e011";
}

.icon-ar-camera:before {
  content: "\e012";
}

@font-face {
  font-family: "material-unityicons";
  src: url("../fonts/experiment/unityicons/generated/unityicons.ttf") format("truetype");
}

.material-icon-experiment [data-icon]:before {
  content: attr(data-icon);
  font-family: "material-unityicons" !important; // @stylint ignore
}

.material-icon-experiment [class^="icon-"]:before,
.material-icon-experiment [class*=" icon-"]:before {
  font-family: "material-unityicons" !important; // @stylint ignore
}

.material-icon-experiment .icon-android:before {
  content: "\e002";
}

.material-icon-experiment .icon-ar-camera:before {
  content: "\e003";
}

.material-icon-experiment .icon-close:before {
  font-family: "unityicons" !important; // @stylint ignore
  content: "\e008";
}

.material-icon-experiment .icon-gdpr:before {
  content: "\e005";
}

.material-icon-experiment .icon-info:before {
  content: "\e006";
}

.material-icon-experiment .icon-info-ar:before {
  content: "\e007";
}

.material-icon-experiment .icon-person:before {
  content: "\e008";
}

.material-icon-experiment .icon-reload:before {
  content: "\e009";
}

.material-icon-experiment .icon-screen-full:before {
  content: "\e00a";
}

.material-icon-experiment .icon-skip:before {
  content: "\e00b";
}

.material-icon-experiment .icon-star:before {
  content: "\e00c";
}

.material-icon-experiment .icon-star {
}

.material-icon-experiment .icon-volume:before {
  content: "\e00d";
}

.material-icon-experiment .icon-volume-mute:before {
  content: "\e00e";
}
