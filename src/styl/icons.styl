@font-face {
  font-family: "unityicons";
  src: url("../fonts/unityicons.ttf") format("truetype");
}

[data-icon]:before {
  content: attr(data-icon);
  font-family: "unityicons" !important; // @stylint ignore
}

[class^="icon-"]:before,
[class*=" icon-"]:before {
  font-family: "unityicons" !important; // @stylint ignore
}


.icon-android:before {
  content: "\e000";
}

.icon-star:before {
  content: "\e001";
}

.icon-info:before {
  content: "\e002";
}

.icon-volume:before {
  content: "\e003";
}

.icon-volume-mute:before {
  content: "\e004";
}

.icon-reload:before {
  content: "\e005";
}

.icon-person:before {
  content: "\e006";
}

.icon-skip:before {
  content: "\e007";
}

.icon-close:before {
  content: "\e008";
}

.icon-screen-full:before {
  content: "\e009";
}
