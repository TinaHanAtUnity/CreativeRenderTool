{
  "extends": "tslint-microsoft-contrib",
  "rules": {
    "no-import-side-effect": false,
    "missing-jsdoc": false,
    "typedef": [false, "call-signature", "arrow-call-signature", "parameter", "arrow-parameter", "property-declaration", "variable-declaration", "member-variable-declaration"],
    "variable-name": false,
    "no-bitwise": false,
    "no-any": true,
    "no-typeof-undefined": false,
    "whitespace": [true, "check-decl", "check-operator", "check-separator", "check-type"],
    "prefer-template": false,
    "no-increment-decrement": false,
    "no-implicit-dependencies": false,
    "no-submodule-imports": false,
    "strict-boolean-expressions": false,
    "no-unsafe-any": false,
    "restrict-plus-operands": false,
    "no-void-expression": false,
    "no-backbone-get-set-outside-model": false,
    "promise-function-async": false,
    "newline-before-return": false,
    "object-literal-key-quotes": [false, "as-needed"],
    "max-line-length": [false, 140],
    "no-floating-promises": false,
    "max-func-body-length": [false, 100, {"ignore-parameters-to-function-regex": "describe"}],
    "no-parameter-reassignment": false,
    "ordered-imports": false,
    "import-name": false,
    "match-default-export-name": false,
    "completed-docs": [false, "classes"],
    "no-inner-html": false,
    "member-ordering": [false, { "order": "fields-first" }],
    "no-suspicious-comment": false,
    "no-non-null-assertion": false,
    "align": [false, "parameters", "arguments", "statements"],
    "no-object-literal-type-assertion": false,
    "no-reserved-keywords": false,
    "function-name": false,
    "no-unnecessary-class": false,
    "cyclomatic-complexity": false,
    "no-for-in": false,
    "no-function-constructor-with-string-args": false,
    "promise-must-complete": false,
    "max-classes-per-file": [false, 3],
    "no-string-based-set-timeout": false,
    "mocha-no-side-effect-code": false,
    "no-http-string": [false, "http://www.example.com/?.*", "http://www.examples.com/?.*"],
    "export-name": false,
    "insecure-random": false
  }
}
